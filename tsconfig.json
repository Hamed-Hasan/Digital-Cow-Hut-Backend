{
    "compilerOptions": {
      "target": "es2021",
      "module": "commonjs",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "declaration": true,
      "outDir": "dist",
      "rootDir": "."
    }
  }
  